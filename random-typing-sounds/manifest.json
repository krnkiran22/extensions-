{
  "manifest_version": 3,
  "name": "Key Sound Player",
  "version": "1.0",
  "description": "Play sounds on specific key presses",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
